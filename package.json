    "dotenv": "^4.0.0",
    "express-jwt": "^5.1.0",
    "mongoose": "^4.8.1",
    "mongoose-deep-populate": "^3.0.0",
    "morgan": "^1.7.0",
    "pathfinder-ui": "^1.0.3",
    "socket.io": "^1.7.2",
    "spdy": "^3.4.4"
  }
