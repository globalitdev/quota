    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2017-node7": "^0.4.1",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "budo": "^9.3.0",
    "envify": "^3.4.1",
    "ftw": "git+ssh://git@bitbucket.org/globalit/fanteam-widgets.git#fantwo",
